{
  "version": 2,
  "builds": [
    {
      "src": "check.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE": "@tenkarstavern_database",
    "SENTRY_DSN": "@tenkarstavern_sentry_dsn",
    "TWITTER_CONSUMER_KEY": "@tenkarstavern_twitter_consumer_key",
    "TWITTER_CONSUMER_SECRET": "@tenkarstavern_twitter_consumer_secret",
    "TWITTER_ACCESS_TOKEN_KEY": "@tenkarstavern_twitter_access_token_key",
    "TWITTER_ACCESS_TOKEN_SECRET": "@tenkarstavern_twitter_access_token_secret"
  },
  "alias": [
    "tenkarstavern-twitterbot"
  ]
}
